function dLinkPage_1(linkPage){
  var platform_filter = /win16|win32|win64/;

    if(!platform_filter.test(navigator.platform.toLowerCase())){
      alert("Windows 환경에서 지원되는 기능입니다.");
      return;
    }
  
    location.href ="https://banking.dgb.co.kr/com_ebz_setup.act";
}