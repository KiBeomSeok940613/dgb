				result += "	<span><img src=\"/img/common/bg/ebz_bg_product_ico3.gif\" alt=\"추천상품\"></span>";
			
			if(row.POP_PD_YN  == "Y" || row.RCMD_PD_YN == "Y" || row.NWGD_YN    == "Y"){
				result += "</li>";
			}
			
			if (row.PD_NM.length > "13")
			{
				addCls = " long_line";
			}
			result += "<li class=\"gnb_visual_area\"><p class=\"product_sub_txt\"><img src=\""+row.IMG_FILE_NM+"\" width=\"224\" height=\"142\" onerror=\"this.style.visibility='hidden'\" alt=\"" + row.PD_NM + " 상품 이미지\"/></p></li>";
			result += "<li class=\"gnb_title_area\"><p class=\"product_sub_txt\">" + row.PD_NM + "</p></li>";


