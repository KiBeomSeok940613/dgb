	dataCtrl.delObjData("MENU_LOCATION");

	var obj = {'MENU_LOCATION' 	: linkValue,
			   'MENU_DEPTH'		: '3',
			   'ID'				: id,
			   'NUM'			: num,
			   'LOCATION_PAGE'	: linkPage,
			   'BT_LINK'	    : 'TRUE',			   
			   'HP_BK'			:'HP',			   
			   'SUB_LINK'		:linksubpage
			  };		
	
	dataCtrl.setObjData("MENU_LOCATION",JSON.stringify(obj));

	location.href = linkPage;
}

