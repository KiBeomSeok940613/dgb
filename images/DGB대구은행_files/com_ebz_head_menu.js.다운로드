					maxRate = jex.null2Void(row.ITN_MAX_RATE);
				}
				else if(row.WIC_JN_PSBLYN == "Y" && row.ITN_JN_PSBLYN != "Y"){
					maxRate = jex.null2Void(row.WIC_MAX_RATE);
				}
				else if(row.SMRT_BNK_JN_PSBLYN == "Y" && row.ITN_JN_PSBLYN != "Y"){
					maxRate = jex.null2Void(row.ITN_MAX_RATE);
				}
